{
  "name": "nuxt-feature-flags",
  "version": "1.1.4",
  "description": "Feature flags for Nuxt with A/B testing and variant support",
  "keywords": [
    "nuxt",
    "feature",
    "flags",
    "ab-testing",
    "variants",
    "rollouts",
    "experiments"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rxb3rth/nuxt-feature-flags.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./build": {
      "types": "./dist/build.d.ts",
      "import": "./dist/build.mjs",
      "require": "./dist/build.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build && npm run client:build",
    "prepare": "nuxi prepare",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare src/runtime/devtools-client",
    "client:build": "nuxi generate src/runtime/devtools-client",
    "client:dev": "nuxi dev src/runtime/devtools-client --port 3300",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release --patch --bump && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "@nuxt/devtools-kit": "^2.6.2",
    "@nuxt/devtools-ui-kit": "^2.6.2",
    "defu": "^6.1.4",
    "sirv": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.15.4",
    "@types/node": "latest",
    "@vitest/coverage-v8": "^2.1.8",
    "changelogen": "^0.5.7",
    "eslint": "^9.20.1",
    "glob": "^11.0.3",
    "h3": "^1.12.0",
    "nuxt": "^3.15.4",
    "pathe": "^2.0.3",
    "typescript": "~5.6.3",
    "vitest": "^2.1.9",
    "vue-tsc": "^2.2.2"
  }
}